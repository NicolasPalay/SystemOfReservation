{% extends 'base.html.twig' %}

{% block title %}Register{% endblock %}

{%  block CssHeader %}ca-form-header{%  endblock %}
{% block headerImg %}<img src="{{ asset('assets/uploads/pictures/icon-inscription.png')}}" alt="">{% endblock %}
{% block headerPara %}<p>
    <h1 class="h3 mb-3 ca-h1-form">Inscription</h1>
    {{ form_errors(registrationForm) }}

    {{ form_start(registrationForm) }}
    {{ form_row(registrationForm.email) }}
    {{ form_row(registrationForm.fullName) }}
    {{ form_row(registrationForm.password) }}
    {{ form_row(registrationForm.agreeTerms) }}


    <button type="submit" class="btn pulse ca-btn">Register</button>
    {{ form_end(registrationForm) }}
</p>{% endblock %}
{% block headerReservation %}{% endblock %}
{% block body %}



{% endblock %}
